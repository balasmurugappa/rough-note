/java/gradle-wrapper.jar
/java/gradle-wrapper.properties
/java/gradlew
/java/gradlew.bat
# IDE-specific files
/java/.idea/
/java/.project
/java/.classpath
/java/.settings/
/java/*.iml
# Logs and temporary files
/java/logs/
/java/*.log
/java/tmp/
/java/.DS_Store
